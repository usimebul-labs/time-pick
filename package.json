{
  "name": "monorepo",
  "private": true,
  "scripts": {
    "build": "pnpm --filter @repo/database db:generate && turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.26.2",
  "engines": {
    "node": ">=18"
  }
}